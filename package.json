{
  "name": "webpack-babel-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -fr dist *.log .DS_Store",
    "webpack-dev": "NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js --progress",
    "webpack-prod": "NODE_ENV=production webpack --config config/webpack.config.prod.js --optimize-minimize --progress",
    "build": "yarn run clean && yarn run webpack-prod",
    "start": "yarn run clean && yarn run webpack-dev"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-lean-imports": "^0.3.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "happypack": "^5.0.0-beta.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.5.3",
    "open-browser-webpack-plugin": "^0.0.5",
    "react-hot-loader": "^4.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2"
  }
